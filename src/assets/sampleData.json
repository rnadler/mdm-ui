              "options": {
                "cpap": "CPAP",
                "vpap": "VPAP",
                "ipap": "IPAP"
              }
            }
          },
          "pressure": {
            "label": "Pressure",
            "formattedValue": "100%",
            "editable": {
              "value": "100",
              "format": "number",
